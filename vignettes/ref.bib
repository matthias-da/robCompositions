@INPROCEEDINGS{BL+82,
  author = {Burg, John Parker and Luenberger, David G. and Wenger, Daniel L.},
  title = {Estimation of structured covariance matrices},
  year = {1982},
  pages = {963-974},
  note = { },
  abstract = {Solves the problem of finding for a given set of vector samples from
	a random process a covariance marix of specified structure that corresponds
	in a reasonable way to the given data. The solution formulation is
	to assume that the random process is zero-mean multivariate Gaussian,
	that the vector samples are independent, and to take as our solution
	the covariance matrix of specified structure that maximizes the probability
	of occurrance of our vector samples. Uses the maximum-entropy method
	of spectral estimation. The solution to the problem comes from maximum
	likelihood considerations and thus should, in some sense, give us
	a reasonable answer, even if the process is not Gaussian and the
	vector samples are not independent.},
