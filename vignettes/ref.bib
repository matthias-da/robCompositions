@ARTICLE{Bar91,
  author = {Barnes, Randal J.},
  title = {The variogram sill and the sample variance},
  journal = {Mathematical Geology},
  year = {1991},
  volume = {23},
  pages = {673-678},
  number = {4},
  resumen = {The relationship between the sill of the variogram and the sample
	variance is explored. It is shown, that the common practice of using
	the sample variance as an estimate of the variogram sill is not always
	appropriate. When the samples are evenly distributed over an areal
	extent many times larger than the range of the variogram, it will
	be safe (rule of thumb: 3 times the range); if the sample values
	are collected within an area with dimension equal to or less than
	the variogram range, then the sample variance will, on the average,
	significantly underestimate the variogram sill. If there is an otherwise
	insignificant low-scale trend and the samples are taken over a large
	area, then the sample variance will, on the average, significantly
	overestimate an apparent sill.}
}

